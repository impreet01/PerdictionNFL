{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Model Summary Artifact",
  "type": "object",
  "required": ["season", "week", "generated_at", "ensemble"],
  "properties": {
    "season": { "type": "integer" },
    "week": { "type": "integer" },
    "generated_at": { "type": "string" },
    "ensemble": {
      "type": "object",
      "required": ["weights"],
      "properties": {
        "weights": {
          "type": "object",
          "additionalProperties": { "type": "number" }
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
